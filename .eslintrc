// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "airbnb/base",
  "env": {
    "es6": true
  },
  "ecmaFeatures": {
    "generators": true
  },
  "rules": {
    "no-param-reassign": 0,
    "no-console": 2,
    "max-statements": [2, 50],
    "max-depth": [2, 5],
    "max-nested-callbacks": [2, 3],
    "max-params": [2, 5],
    "complexity": [1, 10],
    "max-len": [2, 120, 4]
  }
}
